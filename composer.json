{
    "name": "brazilliance/netsuite-connect",
    "description": "A PHP-based connection and authorization library for NetSuite Release 2020.1 and greater.",
    "homepage": "https://github.com/brazilliance/netsuite-connect",
    "license": "MIT",
    "authors": [
        {
            "name": "Miquel Brazil",
            "email": "me@miquelbrazil.com",
            "homepage" : "https://miquelbrazil.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth1",
        "client",
        "authorization",
        "authorisation",
        "netsuite",
        "oracle",
        "endpoint"
    ],
    "require": {
        "php": ">=7.1 || >=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
